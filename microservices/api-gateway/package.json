{
  "name": "api-gateway",
  "version": "1.0.0",
  "description": "API Gateway for the microservices-template",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start:dev": "tsc-watch --onSuccess 'node dist/main.js'",
    "start:debug": "tsc-watch --onSuccess 'node --inspect=0.0.0.0:'${DEBUG_PORT:-9229}' dist/main.js'",
    "prestart": "tsc",
    "start": "node dist/main.js",
    "test": "jest"
  },
  "engines": {
    "node": "^10.15.3"
  },
  "dependencies": {
    "@nestjs/common": "^5.3.0",
    "@nestjs/core": "^5.3.0",
    "@nestjs/microservices": "^5.3.0",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/testing": "^5.3.0",
    "@nestjs/websockets": "^5.3.0",
    "amqp-connection-manager": "^2.3.1",
    "amqplib": "^0.5.1",
    "axios": "^0.18.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.7.2",
    "dotenv": "^6.0.0",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "joi": "^14.3.1",
    "jwks-rsa": "^1.3.0",
    "lodash": "^4.17.11",
    "pino": "^5.6.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typescript": "^3.4.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.4",
    "@types/express": "^4.16.0",
    "@types/express-jwt": "^0.0.40",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/node": "^7.0.41",
    "@types/pino": "^5.6.0",
    "jest": "^23.6.0",
    "pino-pretty": "^2.2.1",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.1",
    "ts-node": "^6.0.0",
    "tsc-watch": "^1.0.29",
    "tsconfig-paths": "^3.6.0",
    "tslint": "^5.9.1"
  }
}
